    template <typename T>
    void testVampireKnown(const std::vector<std::tuple<T, T, T>> &known) {
        for (auto i : known) {
            if (!isVampire(std::get<0>(i), std::get<1>(i), std::get<2>(i))) {
                std::cout << "Error: Expected isVampire(" << std::get<0>(i) << ", "
                << std::get<1>(i)
                << ") = " << (std::get<2>(i) ? "true" : "false")
                << ", but it returned "
                << (endsWithZero(std::get<0>(i), std::get<1>(i)) ? "true"
                    : "false")
                << "\n";
            }
        }
    }
    
