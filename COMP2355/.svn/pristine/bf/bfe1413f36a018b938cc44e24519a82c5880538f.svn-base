// These are internal testing helper functions. They are declared here instead
// of in the header where we would normally declare functions because no one
// other than these testing functions will use them. To avoid potential linker
// errors in the extremely unlikely case that someone else names their functions
// the same as our helper methods, we will use the standard C++ idiom of putting
// them in an anonymous namespace. In effect, this will randomize the name to
// something unpredictable and usable only within this file
//
// The testing functions defined in the header appear at the end of this file

namespace {
    template <typename T> void testHappyTyped();
    template <typename T>
    void testHappyKnown(const T begin, const T end, const std::vector<T> &known,
                        const T radix);
    template <typename T> void testSumOfSquaredDigitsTyped();
    
    template <typename T> void testIntegerToStringTyped();
    template <typename T> void testEndsWithZeroTyped();
    template <typename T> void testIsVampireTyped();
    template <typename T>
    void testVampireKnown(const std::vector<std::tuple<T, T, T>> &known);
} // end anonymous namespace

namespace {
    template <typename T>
    void testHappyKnown(const T begin, const T end, const std::vector<T> &known,
                        const T radix) {
        typename std::vector<T>::size_type next = 0;
        for (T i = begin; i <= end; ++i) {
            bool status = isHappy(i, radix);
            if (i != known[next]) {
                // Not known, so expect false
                if (status)
                    std::cout << "Error: Expected isHappy(" << i << ", " << radix
                    << ") to be false, but it wasn't\n";
            } else {
                // Known, so expect true
                if (!status)
                    std::cout << "Error: Expected isHappy(" << i << ", " << radix
                    << ") to be true, but it wasn't\n";
                
                if (next < known.size() - 1) {
                    // Still more known
                    ++next;
                } else {
                    // Exhausted all known, expect false until end
                    for (++i; i <= end; ++i) {
                        status = isHappy(i, radix);
                        if (status)
                            std::cout << "Error: Expected isHappy(" << i << ", " << radix
                            << ") to be false, but it wasn't\n";
                    }
                }
            }
        }
    }
    
